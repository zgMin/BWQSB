page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;
  margin: 0;
  border-style: border-box;
}
page {
  /* 定义主题颜色 */
  --themeColor: #449aeb;
  /* 定义统一字体大小 大小是 375px 
  1px = 2rpx
  14px = 28rpx
  */
  font-size: 28rpx;

}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
